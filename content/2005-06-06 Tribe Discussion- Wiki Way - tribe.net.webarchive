// Fade effect for new postings
// First seen at http://basecamphq.com

var Color= new Array();
Color[1] = "ff";
Color[2] = "ee";
Color[3] = "dd";
Color[4] = "cc";
Color[5] = "bb";
Color[6] = "aa";
Color[7] = "99";

function waittofade() {
	if (document.getElementById('fade')) {
    setTimeout("fadeIn(7)", 10);
	 }
}

function fadeIn(where) {
    if (where >= 1) {
        document.getElementById('fade').style.backgroundColor = "#ffff" + Color[where];
		  if (where > 1) {
			  where -= 1;
			  setTimeout("fadeIn("+where+")", 200);
			} else {
			  where -= 1;
			  setTimeout("fadeIn("+where+")", 200);
			  document.getElementById('fade').style.backgroundColor = "transparent";
			}
    }
}

// Resize an image to fit it's container
function fitImage(imageId, containerId, nativeWidth) {
  var myDiv = document.getElementById(containerId);
  var myImage = document.getElementById(imageId);
  
  if(myDiv) {

    // take the smaller of offsetwidth and scrollwidth
    if (myDiv.offsetWidth < myDiv.scrollWidth) {
      containerWidth = myDiv.offsetWidth
    } else {
      containerWidth = myDiv.scrollWidth    
    }

    // if the image has been upscaled, reset it to it's native size    
    if(nativeWidth < myImage.offsetWidth) {
      //alert('containerWidth = ' + containerWidth + '\n nativewidth = ' + nativeWidth + '\nimageOffset = ' + myImage.offsetWidth);      
      myImage.style.width = nativeWidth + 'px';
    }
    
  }
}

// sets the onclick action of the toggle div to also call functionName after the toggleDisplay function has been called.
function setToggleAction(toggleId,functionName) {
  var myToggle = document.getElementById('TOG'+toggleId);
  if(myToggle) {    
    var myElement = document.getElementById(toggleId);
    myToggle.onclick = function() { toggleDisplay(toggleId); if (myElement.style.display == "block") { eval(functionName); }  return false; }
  }
}
  
