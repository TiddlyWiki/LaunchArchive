@import url(../../rsc/bd_topnavbar.css);	/* Import blogde topnavbar styles */
@import url(../../rsc/bd_basic.css);	/* Import blogde basic styles */

body {
	background: #fff;
	padding: 0;
	margin: 0;
	text-align: center;
	color: #000;
	font: 80% Arial, Helvetica, sans-serif;
}

/* Page Structure
----------------------------------------------- */
#wrap {
	margin: 0 auto;
	padding: 0;
	width: 780px;
	text-align: left;
}
#header {
	margin: 0 10px 0 10px;
	border-bottom: 1px solid #999999;
}
#content {
	clear: both;
	margin: auto;
	padding: 0;
	width: auto;
}
#left{
	/* two columns, so leave this empty */
}
#right{
	float: right;
	width: 250px;
	margin-right: 10px;
	padding: 20px 0 0 0;
}
#middle{
	margin-left: 0;
	margin-right: 280px;
	padding: 10px;
}
#footer {
	margin: 0;
	padding: 1em;
}

/* links
----------------------------------------------- */
a {
	color: #669999;
	text-decoration: none;
}
a:visited {
	color: #669999;
	text-decoration: none;
}
a:hover {
	color: #7D7C7C;
	text-decoration: underline;
}
a img {
  border-width: 0;
}

/* headings
----------------------------------------------- */
h1 {
	color: #999999;
	font-size: 160%;
}
h2 {
	color: #999999;
	font-size: 140%;
	margin: 0;
}
h3 {
	color: #CC6600;
	font-size: 120%;
}
h4 {
	color: #7D7C7C;
	font-size: 100%;
}

/* #header
----------------------------------------------- */
#header-image {
}
.header-image-link {
}
#header-text {
}
#blog-title h1 {
	margin: 0;
	padding: 10px;
	color: #7D7C7C;
	font-size: 200%;
	font-weight: bold;
}
#blog-title a {
	color: #7D7C7C;
	text-decoration: none;
}
#blog-title a:visited {
	color: #7D7C7C;
	text-decoration: none;
}
#blog-title a:hover {
	color: #7D7C7C;
	text-decoration: none;
}
p.blog-description {
	margin: 0;
	padding: 0 10px 20px 10px;
	color: #7D7C7C;
}
