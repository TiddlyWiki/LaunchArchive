/* #posts
----------------------------------------------- */
#posts {
}
.post {
	margin-bottom: 40px;
	padding: 0px;
	border-bottom: 1px dotted #999999;
}
.post-date-header {
	margin-top: 0;
	border-bottom: 1px solid #999999;
}
.post-friends {
	padding: 5px;
	border: 1px solid #CC6600;
}
.post-title {
	margin-top: 10px;
	padding: 0;
	border: none;
	color: #CC6600;
}
.post-body {
	margin-top: 20px;
	padding: 0;
}
.post-body p {
}
.post-footer {
	margin-top: 30px;
	padding: 0;
	font-size: 90%;
}
.post-item-author {
	font-size: 90%;
	/*display: none;*/
}
.post-item-datetime {
	font-size: 90%;
	/*display: none;*/
}
.post-item-friends {
	color: #000;
	font-weight: bold;
}
.post-item-tags {

}
table.post-item-tags {
	margin: 0;
	padding: 0;
	margin-top: 5px;
	font-size: 100%;
}
table.post-item-tags td {
	margin: 0;
	padding: 0;
	padding-right: 5px;
	border: none;
	vertical-align: top;
}
.post-item-permalink {
}
.post-item-comments {
	font-size: 110%;
	font-weight: bold;
}
.post-item-trackbacks {
	/*display: none;*/
}
.post-item-trackback-rdf {
}
.post-item-emailpost {
}
.post-item-edit {
}
.post-navigation {
	padding-top: 5px;
	font-weight: bold;
	text-align: center;
}


/* #comments
----------------------------------------------- */
div.comment, /* Entire comment block */
form.comment /* New comment form */
{ 
	margin: 1ex 2ex;
	padding: 0 1ex;
	border: 1px dotted #999;
}
.comment-title {
	margin: 1ex 0;
	font-weight: bold;
}
.comment-text {
	margin-top: 0;
	margin-bottom: 0;
}
.comment-text-small {
	font-size: 80%;
	margin: 1ex 0;
}
input.comment, 
textarea.comment
{
	background-color: #fff;
	border: 1px solid #ccc;
	width: 97%;
	padding: 2px;
	margin: 0ex;
}
select.comment		/* Used by the profile form */
{
	background-color: #eee;
	border: 1px solid #ccc;
	padding: 1px;
	margin: 0ex;
}
/* forms: comments and profile */
form.comment fieldset
{	/* one form line */
	border: none;
	margin: 2ex 0ex;
	padding: 0ex;
	/* border: 1px solid #00f; */
}
form.comment fieldset label {
	font-weight: bold;
}
form.comment fieldset div.label
{ /* label of form line */
	/*float: left;
	width: 28%;*/
	text-align: left;
	padding-top: .5ex;
	font-weight: bold;
	/* border: 1px solid #f00; */
}
form.comment fieldset div.input
{ /* input zone of form line */
	margin: 0 0 0 0;
	/* border: 1px solid #f00; */
}
form.comment fieldset.input{
	margin: 0;
	/* border: 1px solid #0f0; */
}
form.comment fieldset div.info
{ /* info zone of form line */
	margin: 0 0 0 0;
	padding-top: .5ex;
	/* border: 1px solid #f00;*/
}

/* #footer
----------------------------------------------- */
#footer p {
	margin: 0;
	padding: 0;
	
}
/* #disclaimer
----------------------------------------------- */
#disclaimer {
	margin: 0;
	padding: 20px 0 10px 0;
	background: #fff;
	text-align: center;
	font-size: 80%;
}

/* #bloglists
----------------------------------------------- */
#bloglist {
}
.bloglist-table th {
	text-align: right;
	font-weight: bold;
}

/* #imprint
----------------------------------------------- */
#imprint {
}

/* forms
----------------------------------------------- */
input.search {
	margin-bottom: 1ex;
	padding: 0;
	background-color: #fff;
	border: 2px solid #ccc;
}
input.submit,
input.reset {
	margin: 4px 0;
	padding: 1px;
	background-color: #ddd;
	color: #78a;
	border: 1px solid #000;
	font-weight: bold;
}

input.submit:hover {
	color: #000;
	text-decoration: underline;
}


.bPostfriends {
	/* border: 1px solid #e1ddce; */
	color: #f39;
}

.bPostfriendsBG {
	/* border: 1px solid #e1ddce; */
	background-color: #ff9;
