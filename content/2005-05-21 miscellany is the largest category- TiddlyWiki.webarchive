	padding:0px;
	}
p {
	font:11px/20px verdana, arial, helvetica, sans-serif;
	margin:0px 0px 16px 0px;
	padding:0px;
	}
.Content>p {margin:0px;}
.Content>p+p {text-indent:30px;}


a {
	color:#930;
	font-size:11px;
	font-family:verdana, arial, helvetica, sans-serif;
	text-decoration:none;
