function tsaLiveReqDoReq(query) {
	if (tsaLiveReqLast != query) {
		if (tsaLiveReq && tsaLiveReq.readyState < 4) {
			tsaLiveReq.abort();
		}
		
		if (window.XMLHttpRequest) {
