/* Preliminary Styling
----------------------------------------------- */

* {
margin: 0;
padding: 0;
}

body {
margin: 0;
padding: 0;
height: 100%;
font: normal 13px "Lucida Sans", Verdana, sans-serif;
line-height: 1.3em;
color: #333;
background-color: #fff;
text-align: center;
}

a, a:link {
color: #333;
}

a:active {
color: #999;
}

a:hover {
color: #E95226;
}

h1, h2, h3 {
margin: .5em 0;
}

blockquote {
background: url(http://www.kartooner.com/blog/images/quotebullet.gif) no-repeat;
color: #666;
padding-left: 32px;
margin: .5em 0;
}

h1 a, h3 a {
text-decoration: none;
}

h3.post-title {
display: inline;
font: bold 22px Arial;
letter-spacing: -1px;
}

p {
margin: .7em 0;
}

ol, ul {
margin: 1em 2em;
}

ol li, ul li {
margin: .2em 0;
}

.extlink {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/extlink.png) no-repeat 0 0.4em;
padding-left: 8px;
}

/* Header
----------------------------------------------- */

#infobar {
margin: 0;
padding: .3em;
background: #DEDECD;
border-bottom: 1px solid #F8F8EA;
color: #666;
height: 20px;
}

#infobar p {
margin: 0;
padding: 0;
font-size: 13px;
}

#infobar #searchform {
margin: 0;
padding: 0;
position: absolute;
top: .5%;
right: 1%;
}

#branding {
height: 140px;
margin-bottom: 1em;
background: #EEEEDD url(http://www.kartooner.com/wp-content/themes/kartooner/images/kartoonerdotcom-head.png) no-repeat 50%;
border-bottom: 2px solid #D6D6C5;
}

#branding h1 {
margin: 0;
}

#branding h1 a {
display: none;
}

/* Layout
----------------------------------------------- */

#container {
margin: 20px;
width: 800px;
height: 100%;
margin: 0 auto;
text-align: left;
}

#main-content {
width:400px;
max-width: 400px;
margin-right: 15px;
margin-bottom: 30px;
display:inline;
float:right;
}

#main-content textarea {
font: 1em Arial, Verdana, sans-serif;
color: #333;
width: 100%;
padding: .2em;
}

#main-content input {
padding: .2em;
}

.next-page {
border-top: 1px solid #eee;
border-bottom: 1px solid #eee;
margin: .8em 0;
padding: .1em;
font-size: .9em;
}

#footer {
clear: both;
font-size: 11px;
border-top: 1px solid #eee;
}

/* Date and Comments
----------------------------------------------- */

.date {
margin-right: .9em;
font: normal .9em "Lucida Sans Unicode";
display: block;
float: left;
color: #666;
border-right: 1px solid #ccc;
border-bottom: 1px solid #ccc;
background: #f7f7f7;
padding: .2em .3em .2em .2em;
text-align: right;
}

#addcomment {
display: block;
width: 95%;
color: #fff;
text-decoration: none;
border-bottom: 3px solid #ddd;
}

a#addcomment {
padding-left: 18px;
background: #87AAD9 url(http://www.kartooner.com/blog/images/arrow_down.gif) 0 0.2em no-repeat;
}

#addcomment:hover {
background: #9AB8DF url(http://www.kartooner.com/blog/images/arrow_down.gif) 0 0.2em no-repeat;
}

ol#commentlist {
margin: 0;
padding: 0;
width: 100%;
}

ol#commentlist li {
margin: 1em 0;
list-style: none;
}

.comment-text {
margin-right: 0px;
margin-left: 80px;
}

div.comment p {
margin: .5em 0;
}

.comment-text p {
margin: 0;
}

.comment-text p span.author a {
font-weight: bold;
text-decoration: none;

}

.comment-author-gravatar {
float: left;
width: 80px;
}

#commentlist .comment-text { 
margin-bottom: 25px; 
}

.comment-area {
background: transparent;
}

p.commentmetadata {
font-size: 10px;
margin-bottom: 5px;
background: #f7f7f7 url(http://www.kartooner.com/blog/images/comment.gif) .1em no-repeat;
padding-left: 20px;
}

.commentmetadata a {
color: #777;
text-decoration: none;
}

.commentmetadata a:hover {
color: #555;
}

.align-right {
float: right;
}

/* Posts
----------------------------------------------- */

.post-content {
margin: 1.5em 0;
}

.post-content p {
font-size: 12px;
}

.entry h3 {
font-size: 13px;
}

.post-teaser {
margin: 5px 0;
font-weight: bold;
text-decoration: none;
}

.entry {
margin-bottom: 15px;
}

#postinfo {
margin-bottom: 15px;
padding-top: 2px;
}

#postinfo p, #postinfo p a:link {
color: #777;
font-size: 11px;
line-height: 1.3em;
}

#postinfo p a:hover {
color: #222;
}

#postinfo img {
float: left;
margin-top: 2px;
margin-right: 5px;
}

#main-content #featured-article {
margin: 0;
padding: 5px 0 0 0;
}

#main-content #featured-article #title-wrap {
height: 35px;
padding: 0px;
}

#main-content #featured-article .date {
border: none;
background: transparent;
float: right;
margin: 0;
}

#main-content #featured-article .post-content {
margin: 0;
padding: 0;
width: 100%;
}

#main-content h3#archives {
background: #EAD695;
border: 1px solid #C1A441;
border-bottom: none;
color: #706443;
margin: 10px 0 0px 0;
padding-left: 5px;
font-size: 13px;
}

ol#archived {
margin: 0;
padding: 0;
border: 1px solid #C1A441;
list-style-type: none;
}

ol#archived li.archive-link {
margin: 0;
padding: 3px 0 0 4px;
display: block;
background: #EEDEAA;
}

ol#archived li.archive-link.alt {
background: #F2E6BF;
}

ol#archived li.archive-link a {
text-decoration: none;
color: #60563A;
padding-left: 2px;
}

ol#archived li.archive-link a:hover {
color: #E95226;
}

#main-content .postspermonth {
margin: 10px 0 10px 0;
list-style: none;
padding-left: 25px;
}

#main-content .postspermonth a {
text-decoration: none;
}

/* Sidebar
----------------------------------------------- */

#sidebar {
width: 160px;
max-width: 160px;
float: right;
margin-right: 1em;
margin-bottom: 30px;
}

#sidebar img {
margin: 5px 0;
}

#sidebar h2 {
font: bold 15px Arial;
text-align: left;
}

#sidebar p {
font-size: 10px;
padding-left: 8px;
width: 160px;
max-width: 160px;
line-height: 11px;
}

#sidebar ul {
margin: 0;
padding: 0;
}

#sidebar ul li {
font-size: 11px;
display: block;
margin: 0;
text-decoration: none;
padding-left: .5em;
text-align: left;
}

#sidebar ul li a {
text-decoration: none;
}

#sidebar ul li a:hover {
background: #f7f7f7;
}

h2#navigate {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/red-bullet.png) no-repeat 0.4em;
padding-left: .9em;
}

h2#friends {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/purple-bullet.png) no-repeat 0.4em;
padding-left: .9em;
}

h2#links {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/blue-bullet.png) no-repeat 0.4em;
padding-left: .9em;
}

h2#meta {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/green-bullet.png) no-repeat 0.4em;
padding-left: .9em;
}

/* Right Column
----------------------------------------------- */

#right-col {
width: 170px;
max-width: 170px;
float: right;
margin-left: 1em;
}

#right-col h2 {
color: #333;
font: 15px Arial;
font-weight: bold;
}

#right-col p {
font-size: 11px;
margin-left: 5px;
line-height: 1.4em;
}

#right-col ul {
margin: 0 1em 0 0;
padding: 0;
list-style: none;
}

#right-col ul li {
font-size: 11px;
padding-left: .5em;
}

#right-col ul li a {
text-decoration: none;
}

h2#recentarticles {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/yellow-bullet.png) no-repeat 0.4em;
padding-left: .9em;
}

h2#categories {
background: url(http://www.kartooner.com/wp-content/themes/kartooner/images/orange-bullet.png) no-repeat 0.4em;
padding-left: .9em;
}

.left-image {
float: left;
background: #fff;
margin: 0 .5em .5em 0;
border: 1px solid #eee;
padding: .3em;
}

ul.children a:link {
color: #666;
}

ul.children a:hover {
color: #E95226;
}

.right-image {
float: right;
background: #fff;
margin: 0 0 .8em .5em;
border: 1px solid #eee;
padding: .3em;
}

/* Delicious
----------------------------------------------- */

ul#delicious {
margin-bottom: 10px;
}

ul#delicious li {
border-bottom: 1px solid #eee;
}

ul#delicious li a {
display: block;
}

ul#delicious li a:hover {
background: #f7f7f7;
}
