/* orientation 2c left  */

#container {
  float: right;
  margin: 0px 0px 0px -210px;
  width: 100%;
}

#content {
  margin: 0px 10px 0px 240px;
}

#sidebar01 {
  float: left;
  width: 210px;
}

/* orientation fixed width - center  */

body {
  text-align: center;
}

#wrapper {
  margin: 0px auto;
  width: 750px;
}

/* admin styles */

/* messages */
.message {
  background-color: #FFEC03;
  border: 1px solid #FF7006;
  color: #A21A1A;
  padding: 0.2em 0.8em 0.15em 34px;
  background: url("http://twoday.net/static/icon_message_24.gif") 2px -1px no-repeat #FFEC03;
  margin: 1em 0;
}

div.admin ul li {
  list-style: none;
  background: url("http://twoday.net/static/bg_listicon16.gif") 0 0px no-repeat;
  padding: 0 0 5px 22px;
}

div.admin ul {
  margin: 0 0 1em 0;
  padding: 0;
}

.listItem ul.taskList li {
  background: url("http://twoday.net/static/bg_listicon16_simple.gif") 0 2px no-repeat;
}

.listNavigation {
  margin-top: 1.5em;
  border-bottom: 1px solid #ffd29d;
  padding-bottom: 2px;
  padding-left: 7px;
}

.listNavigation span {
  color: #ffd29d;
}

.listNavigation a.active {
  font-weight: bold;
}

div.admin small, 
div.admin .small {
  font-size: 13px;
  color: #ffd29d;
}

/* lists */

div.pageNav {
  margin: 5px 0 13px 0;
}

div.pageNavSummary {
  /* float: left; */
}

div.pageNavBar {
  /* float: right; */
  text-align: right;
}

span.pageNavItem,
span.pageNavSelItem  {
  margin-left: 3px;
  margin-right: 3px;
}

table.listItem td.leftCol {
  width: 70%;
  vertical-align: top;
}

table.listItem td.rightCol {
  width: 30%;
  vertical-align: top;
}


/* forms */

div.admin fieldset {
  -moz-border-radius: .8em;
  border: 1px solid #ffd29d;
}

div.admin fieldset legend {
  font-weight: bold;
  font-size: 1em;
  left: 1em;
  padding: 0 0.6em;
  color: #fdc27d;
}

div.admin fieldset.formList {
  padding: 0 1em 0 1.5em;
}

div.admin .formList p {
  border-bottom: 1px solid #ffd29d;
  margin-top: 0.5em;
  padding-bottom: 0.8em;
}

div.admin label {
  font-size: 1em;
  font-weight: bold;
  font-color: #ffd29d;
}

.formText {
  width: 95%;
  font-family: Trebuchet MS, Verdana, Arial;
  font-size: 14px;
  font-weight: normal;
}

.formWide {
  width: 90%;
  font-family: Trebuchet MS, Verdana, Arial;
  font-size: 14px;
  font-weight: normal;
}

div.admin .intend {
  display: block;
  margin-left: 1.8em;
}

p.buttons {
  margin-top:3em;
  text-align: right;
  clear: both;
}

p.buttons input {
  padding: 0 1em;
  text-align: center;
}


body {
  margin: 0px;
  padding: 0px;
  background-color: #800203;
  font-family: Trebuchet MS, Verdana, Arial;
  color: #ffd29d;
  font-size: 14px;
}

a {
  color: #ff9f3f;
  text-decoration: none;
}

a:hover {
  color: #ffd29d;
}

td {
  vertical-align: top;
  font-family: Trebuchet MS, Verdana, Arial;
  color: #ffd29d;
  font-size: 14px;
}

p {
  margin: 6px 0px 6px 0px; 
}

img {
  border: 0;
}


/* wrapper */

#wrapper {
  text-align: left;
}

/* header */

#header {
  background-image: url("http://twoday.net/static/ellyrianna/layouts/rosa/header.jpg"); 
  background-position: center;
  background-color: #ffd29d; 
  border-bottom: none;
  text-align: center;
  margin-top: 18px;
  height: 132px;
}

#header h1 {
  margin: 0;
  padding: 32px 0 0 0;
  font-size: 50px;
  font-family: Trebuchet MS, Verdana, Arial;
}

#header h1 a, #header h1 a:hover {
  color: #ffd29d;
  border: 0;
  border-bottom: none;
}

#header div.subheader {
  font-size: 14px;
  color: #ffd29d;
  margin: 0 0 0 0;
  text-align: right;
  padding-right: 145px;
  padding-top: 10px;
}


/* pagebody */

#pagebody {
  margin: 32px 0px 0px 0px;
}


/* content */

#content {
  line-height: 1.3em;
  padding: 5px 0px 32px 0px;
  overflow: hidden;
  width: 100%; 
}

body>#wrapper #content {
  overflow: visible;
  width:auto;
}

#content h1, #content h2 {
  font-family: Trebuchet MS, Verdana, Arial;
  color: #fdc27d;
  font-size: 16px;
  padding: 0px;
  margin: 0px 0px 18px 0px;
}

.pagelinkTop, .pagelinkBottom {
  padding: 0px 0px 10px 0px;
}

small, .small {
  font-family: Trebuchet MS, Verdana, Arial;
  font-size: 13px;
  color: #ffd29d;
}


/* story */

.story {
  margin: 0px 0px 28px 0px;
  text-align: justify;
}

.story:hover span {
  visibility: visible;
}

h3 {
  font-family: Trebuchet MS, Verdana, Arial;
  color: #fdc27d;
  font-size: 16px;
  margin: 0px 0px 13px 0px;
  padding: 0px;
  border-bottom: 1px dotted #ffd29d;
}

.storyContent {
  padding: 0px 0px 13px 0px;
}

.storyContent cite {
  margin: 0px 0px 0px 20px;
  font-style: italic;
  display: block;
}

.storyDate, .storyLinks, .commentLinks, .commentDate {
  text-align: right;
  font-family: Trebuchet MS, Verdana, Arial;
  font-size: 13px;
  color: #ffd29d;
}

.storyDate span, .storyLinks span {
  visibility: hidden;
}


/* story images */

.storyContent .left {
  float: left;
  margin: 7px 10px 7px 0px;
}

.storyContent .center {
  display: block;
  text-align: center;
  margin: 7px auto 7px auto;
}

.storyContent .right {
  float: right;
  margin: 7px 0px 7px 10px;
}


/* comments */

.comments {
  margin: 10px 0px 0px 0px;
}

.comments h4 {
  font-size: 14px;
  padding: 0px;
  margin: 0px 0px 2px 0px;
}

.commentLinks, .commentDate {
  text-align: left;
  margin: 0px 0px 2px 0px;
}

.reply {
  margin: 10px 0px 0px 30px;
}


/* form */

form {
  margin: 0px;
  padding: 0px;
}

fieldset {
  padding: 5px 0px 5px 8px;
}

fieldset legend {
  padding: 0px 8px 0px 8px;
}

input, textarea, select {
  font-family: Trebuchet MS, Verdana, Arial;
  color: #ffd29d;
  font-size: 13px;
  background: transparent;
}

.searchbox {
  border: 1px solid #ffd29d;
  background: transparent;
  font-family: Trebuchet MS, Verdana, Arial;
  color: #ffd29d;
  font-size: 13px;
}

.formTitle, .formText  {
  width: 420px;
  border: 1px solid #ffd29d;
}


/* sidebar */

.sidebarItem {
  font-family: Trebuchet MS, Verdana, Arial;
  font-size: 13px;
  color: #ffd29d;
  background: url("http://twoday.net/static/layouts/twodayclassic1/ring.gif") 11px 2px  no-repeat;
  border-bottom: 1px dotted #ffd29d;
  line-height: 1.1em;
  padding: 0px 0px 10px 25px;
  margin: 0px 0px 12px 0px;
}

.sidebarItem td {
  font-size: 13px;
}

.sidebarItem h4 {
  font-family: Trebuchet MS, Verdana, Arial;
  color: #fdc27d;
  font-size: 14px;
  text-transform: lowercase;
  padding: 0px 0px 7px 0px;
  margin: 0px;
}

.sidebarItem ul {
  margin: 0px;
  padding: 0px;
}

.sidebarItem ul li {
  list-style: none;
}

.historyItem {
  margin: 0px 0px 6px 0px;
}

td.calender {
  white-space: nowrap;
}


/* footer */

#footer {
  background-image: url("http://twoday.net/static/layouts/twodayclassic1/footer.jpg");
  background-repeat: repeat-x;
  background-color: #ffd29d;
  border-top: 1px dotted #ffd29d;
  height: 50px;
}


div.clearall {
  clear: both;
  height: 0px;
  font-size: 0px;
