#header
{
	font-family: georgia, verdana, arial, Sans-Serif;
	color: #FF8000;
	font-weight: bold;
	border-left: 1px dotted #FFF;
	border-right: 1px dotted #FFF;
	border-top: 1px dotted #FFF;
	background: #FF9933;
	padding: 20px;
	margin: 0;
}

#logoarea
{
	width: 100%;
	height: 100px;
	border: 2px dotted #993300;
	padding: 2px;
	position: relative;
}
#logoimage
{
	position: absolute;
	right: 10px;
	padding: 2px;
}
#title
{
	color: #fff;
	font-size: 2em;
	text-align: left;
	font-weight: bold;
	margin-bottom: 5px;
}

#subtitle
{
	font-family: georgia, verdana, arial, Sans-Serif;
	color: #FFFFFF;
	font-size: 18px;
	font-weight: bold;
	background: #993300;
	text-align: center;
	clear: left;
	width: 330px;
}

#mainmenuarea
{
	border-top: 1px dotted #993300;
	border-bottom: 1px dotted #993300;
	background: #FFCC66;
	margin-top: 5px;
	margin-right: 20px;
}

.adminNavbarStyle
{
	margin: 0;
	padding: 5px;
	padding-left: 15px;
	list-style-type: none;
}

.adminNavbarStyle LI
{
	border-top: solid;
	border-bottom: solid;
	border-width: 1px;
	padding-top: 1px;
	border-color: #993300;
	background-color: #FFEAC0;
	font-size: 0.9em;
	margin-top: 6px;
	margin-bottom: 6px;
}

.adminNavbarStyle LI A
{
	color: #993300;
	font-family: verdana, arial, Sans-Serif;
	text-decoration: none;
}
.adminNavbarStyle LI A:link
{
	color: #993300;
	text-decoration: none;
}
.adminNavbarStyle LI A:visited
{
	color: #993355;
	text-decoration: none;
}
.adminNavbarStyle LI A:active
{
	color: #FF0000;
}
.adminNavbarStyle LI A:hover
{
	color: #FF0000;
}

.adminNavbarStyle a
{
	margin: 5px;
	color: #fff;
	text-decoration: none;
}

.error
{
	color: #c00;
}

