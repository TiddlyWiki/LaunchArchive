}

.articlesub
{
	color: #666;
	font-size: 0.8em;
	border-top: dashed 1px #ccc;
}

.articlecontent
{
	margin-left: 0;
	margin-bottom: 0;
	margin-right: 0;
	margin-bottom: 10px;
	padding-left: 25px;
	padding-top: 13px;
	background-image: url(../Images/bgitem.gif);
	background-repeat: no-repeat;
}

.articlecomments
{
	margin: 0px;
